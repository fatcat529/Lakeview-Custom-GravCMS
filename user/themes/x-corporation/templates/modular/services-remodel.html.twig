<section class="services-remodel" id="remodeling">
    <div class="remodel-container">
        <div class="remodel-content">
            <div class="title-content">
                <h1 class="title">{{ page.header.headtitle|raw }}</h1>
                <span class="title-indicator"></span>
            </div>
            <div class="remodel-desc">
                <p class="description">{{ page.header.description|raw }}</p>
            </div>
        </div>
        <div class="remodel-img-container">
            <div class="rectangle"></div>
            <div class="remodel-img-content">
                {% for model in page.header.models %}
                    <img class="remodel-img" src="{{ page.media.images[model.image].url }}">
                {% endfor %}
            </div>
        </div>
    </div>
</section>
